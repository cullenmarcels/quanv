page{
    background: #fff;
}

.bottom-nav{
    width: 100%;
    height: 120rpx;
    background: #fff;
    position: fixed;
    bottom: 0;
    z-index: 999;
    border-top: 2rpx solid #ddd;
    font-size: 36rpx;
    color: #8896B1;
}

.bottom-nav view{
    flex: 1;
    text-align: center;
    line-height: 120rpx;
}


.gift{
    width: 40rpx;
    height: 40rpx;
    vertical-align: middle;
    margin-right: 10rpx;
}

.top{
    padding: 30rpx;
    background:#02c6dc;
    color: #fff;
    font-size: 32rpx;
    position: relative;
}

.top-l{
    flex: 1;
}

.top-l-img{
    width: 150rpx;
    height: 150rpx;
    border-radius: 50%;
    border: 4rpx solid #fff;
    /* background: #8896B1; */
    position: relative;
}

.v{
    width: 50rpx;
    height: 50rpx;
    border-radius: 50%;
    /* background: #FFB82C; */
    position: absolute;
    right: -10rpx;
    bottom: -10rpx;
    text-align: center;
   
}

.top-c{
    flex: 2.5;
    padding-top: 20rpx;
}

.top-c view{
    display: -webkit-box;
    word-break: break-all;
    text-overflow: ellipsis;
    overflow: hidden;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:1;
}


.top-r{
    position: absolute;
    right: 30rpx;
    top: 50rpx;
    z-index: 999;
    
}

.attention{
    width: 100rpx;
    height: 50rpx;
}

.quantity{
    margin: 40rpx 0;
    font-size: 28rpx;
}

.quantity-l{
    flex: 1.5;
    text-align: left;
}

.quantity-c{
    flex: 1.6;
    text-align: center;
}

.quantity-r{
    flex: 1.6;
    text-align: right;
}

.speciality-l{
    flex: 1;
    line-height: 50rpx;
}

.speciality-r{
    flex: 6;
    line-height: 50rpx;
    display: -webkit-box;
    word-break: break-all;
    text-overflow: ellipsis;
    font-size: 32rpx;
    overflow: hidden;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:2;
}

.speciality-r-li{
    height: 60rpx;
    line-height: 40rpx;
    font-size: 28rpx;
    border: 2rpx solid #fff; 
    padding: 10rpx 20rpx;
    box-sizing: border-box;
    margin-right: 20rpx;
    margin-bottom:20rpx;
    border-radius: 30rpx;
}

.nav-li{
    flex: 1;
    text-align: center;
    font-size: 34rpx;
    color: #122d64;
    background: #fff;
}

.nav-li view{
    display: inline-block;
    margin: 0 auto;
    line-height: 120rpx;
}

.active{
    color:#41C7D6;
    font-weight: 600;
    border-bottom: 4rpx solid #02c6dc;
}


.ul{
    flex-wrap: wrap;
    justify-content: space-around;
    border-bottom: 2rpx solid #ccc;
    background: #fff;
    margin: 20rpx 0;
}

.li{
    width: 25%; 
    height: 210rpx;
}

.ul .text{
    font-size: 28rpx;
    color: #122d64;
    text-align: center;
}

.img{
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    margin: 30rpx auto 20rpx;
    box-sizing: border-box;
}

.img-t{
    width: 100%;
    height: 100%;
}

.img-img{
    width: 60%;
    height: 60%;
    margin: 20%;
}

.advertising{
    padding: 20rpx 0;
    background: #fff;
    border-bottom: 2rpx solid #ccc;
}

.advertising-img{
    width: 100%;
    height: 300rpx;
    margin-top: 40rpx;
    /* background: #D4F0F3; */
}

