    top: 60rpx;
    left: 5%;
    font-size: 40rpx;
    font-weight: 600;
    color: #fff;
    display: -webkit-box;
    word-break: break-all;
    text-overflow: ellipsis;
    overflow: hidden;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:1;
}

.man{
    width: 90%;
    font-size: 32rpx;
    color: #fff;
